<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/y"
    tools:context="com.example.shitalmam.shit.studyroom">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView38"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="104dp"
        app:srcCompat="@drawable/lp"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="50dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        app:srcCompat="@drawable/i" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView10"
        android:layout_centerHorizontal="true"
        android:text="Study Room"
        android:textColor="@color/colorAccent"
        style="?attr/autoCompleteTextViewStyle"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView45"
        style="?attr/autoCompleteTextViewStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView10"
        android:layout_below="@+id/imageView8"
        android:layout_marginTop="68dp"
        android:text="Open Time:" />

    <TextView
        android:id="@+id/textView46"
        style="?attr/autoCompleteTextViewStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView45"
        android:layout_below="@+id/textView45"
        android:layout_marginTop="51dp"
        android:text=" Close time:" />

    <TextView
        android:id="@+id/textView47"
        style="?attr/autoCompleteTextViewStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView45"
        android:layout_marginStart="25dp"
        android:layout_toEndOf="@+id/imageView8"
        android:text="6 Am" />

    <TextView
        android:id="@+id/textView48"
        style="?attr/autoCompleteTextViewStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView47"
        android:layout_alignTop="@+id/textView46"
        android:text="9pm" />

</android.widget.RelativeLayout>
